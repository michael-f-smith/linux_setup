### System Aliases
alias c="cd .. && ls"
alias vba="vi ~/.bash_aliases"
alias nba="nano ~/.bash_aliases"
alias sba="source ~/.bash_aliases"
alias vb="vi ~/.bashrc"
alias nb="nano ~/.bashrc"
alias sb="source ~/.bashrc"
alias cc="colcon build --symlink-install --continue-on-error --executor sequential"

